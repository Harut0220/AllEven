<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link rel="stylesheet" href="/styles/bootstrap@5.3.0.css">
<link rel="stylesheet" href="https://chatapi.trigger.ltd/Public/manifest.json">

<meta property="og:site_name" content="AllEven">
<meta property="og:title" content="{{title}}" />
<meta property="og:description" content="{{#if description}}{{description}}{{else}}{{serviceName}}{{/if}}">
<meta property="og:image" itemprop="image" content="https://chatapi.trigger.ltd{{#if imageHeader}}/{{imageHeader}}{{else}}/default-event-image.jpg?width=800&height=450{{/if}}" />
<meta property="og:image:secure_url" itemprop="image" content="https://chatapi.trigger.ltd{{#if imageHeader}}/{{imageHeader}}{{else}}/default-event-image.jpg{{/if}}" />
<meta property="og:image:width" content="1200">
<meta property="og:image:height" content="630">
<meta property="og:type" content="website" />



<meta name="twitter:title" content="{{title}}">
<meta name="twitter:description" content="{{category}}">
<meta name="twitter:image" content="https://chatapi.trigger.ltd{{#if imageHeader}}/{{imageHeader}}{{else}}/default-event-image.jpg?width=800&height=450{{/if}}">
<meta name="twitter:card" content="summary_large_image">